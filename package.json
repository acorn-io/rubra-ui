{
  "name": "@gptscript-ai/studio",
  "author": "Acorn Labs <info@acorn.io>",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "main": ".output/electron/background.js",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate",
    "clean": "scripts/clean",
    "download": "node scripts/download.mjs",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix .",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && electron-builder install-app-deps",
    "transpile-electron": "tsc -p ./electron --outDir .output/electron",
    "dev:electron": "NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess 'electron ./.output/electron/background.js'\"",
    "dev:electron:win": "set NODE_ENV=development& concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\"",
    "build:electron": "yarn build && yarn transpile-electron && node build.js"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.18.0",
    "@gptscript-ai/gptscript": "*",
    "@heroicons/vue": "^2.1.3",
    "@nuxt/ui": "^2.16.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/busboy": "^1.5.4",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/markdown-it": "^14.1.1",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@vueuse/core": "^10.9.0",
    "ansi-to-html": "^0.7.2",
    "busboy": "^1.6.0",
    "concurrently": "^8.2.2",
    "dayjs": "^1.11.10",
    "electron": "^30.0.6",
    "electron-builder": "^24.13.3",
    "eslint": "^9.2.0",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "pinia-generic": "^0.2.0",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "tailwindcss": "^3.4.3",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.17",
    "wait-on": "^7.2.0",
    "yup": "^1.4.0"
  },
  "dependencies": {
    "@nuxtjs/color-mode": "^3.4.1",
    "electron-log": "^5.1.4",
    "electron-updater": "^6.1.8",
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  }
}
